{
  "expo": {
    "name": "B-Smart",
    "slug": "budget-management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/bsmart-ico.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logos/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/bsmart-ico.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.instaloknahh.budgetmanagement"
    },
    "web": {
      "favicon": "./assets/logos/bsmart-ico.png"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-image-picker",
        {
          "photosPermission": "Selecct and upload images.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "scheme": "acme",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "43492717-4e0c-4a30-a511-3ae370faa16d"
      }
    }
  }
}
